---
import { Picture } from "@astrojs/image/components";
import Bgimage from "~/assets/backgroundv1-blur.png";

const widths = [450, 800, 1200];
const sizes = "100vw";
---

<Picture
  class="h-full w-full object-cover relative"
  src={Bgimage}
  widths={widths}
  sizes={sizes}
  alt="The ridged surface of the moon"
/>
