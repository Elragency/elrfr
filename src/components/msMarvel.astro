---
import { Picture } from "@astrojs/image/components";
import computer from "~/assets/computer.png";

const widths = [450, 800, 1200];
const sizes = "100vw";
---

<Picture
  class="h-full w-full object-cover"
  src={computer}
  widths={widths}
  sizes={sizes}
  alt="The ridged surface of the moon"
/>

